	<tables>
		<table name="referer_remote_statistics" />
	</tables>
	<columns>
		<column name="count" var="count" default="plus(1)" />
	</columns>
	<conditions>
		<condition operation="equal" column="remote" var="remote" notnull="notnull" />
	</conditions>
</query>
