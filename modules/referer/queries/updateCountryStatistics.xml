<query id="updateCountryStatistics" action="update">
	<tables>
		<table name="referer_country_statistics" />
	</tables>
	<columns>
		<column name="count" var="count" default="plus(1)" />
	</columns>
	<conditions>
		<condition operation="equal" column="country_code" var="country_code" notnull="notnull" />
	</conditions>
</query>
